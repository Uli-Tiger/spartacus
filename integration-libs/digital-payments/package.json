    "@angular/router": "^15.2.4",
    "@ng-select/ng-select": "^10.0.3",
    "@spartacus/cart": "6.4.0",
    "@spartacus/checkout": "6.4.0",
    "@spartacus/core": "6.4.0",
    "@spartacus/schematics": "6.4.0",
    "@spartacus/storefront": "6.4.0",
    "rxjs": "^6.6.0"
  },
